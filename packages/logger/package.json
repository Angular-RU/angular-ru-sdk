{
    "name": "@angular-ru/logger",
    "version": "12.21.3",
    "description": "Angular-RU package",
    "keywords": [
        "angular-ru",
        "angular",
        "logger",
        "sdk"
    ],
    "homepage": "https://github.com/Angular-RU/angular-ru-sdk/tree/master/packages/logger#readme",
    "bugs": {
        "url": "https://github.com/Angular-RU/angular-ru-sdk/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Angular-RU/angular-ru-sdk.git"
    },
    "license": "MIT",
    "author": "splincode <omaxphp@yandex.ru>",
    "scripts": {
        "build:app": "ng build integration --prod",
        "build:lib": "ng build logger && cp README.md dist/logger",
        "deploy": "cp README.md dist/integration && ng deploy --no-build",
        "start": "ng serve",
        "test": "yarn jest --config ./jest.config.js",
        "postversion": "npe version \"$(npe version)\" --package=\"$PWD/dist/logger/package.json\""
    },
    "devDependencies": {
        "devtools-detect": "3.0.0"
    },
    "peerDependencies": {
        "@angular/common": ">=9.1.11",
        "@angular/core": ">=8.0.0"
    },
    "publishConfig": {
        "access": "public",
        "directory": "dist/logger"
    }
}
