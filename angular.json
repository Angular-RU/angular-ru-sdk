{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "defaultProject": "common-library",
    "newProjectRoot": "packages",
    "projects": {
        "common-library": {
            "prefix": "",
            "root": ".",
            "projectType": "library",
            "sourceRoot": "packages/common",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "packages/common/tsconfig.lib.json",
                        "project": "packages/common/ng-package.json"
                    }
                }
            }
        },
        "common-app": {
            "prefix": "",
            "root": ".",
            "projectType": "application",
            "sourceRoot": "packages/common/integration/apps/common",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "packages/common/dist/integration/apps/common",
                        "tsConfig": "packages/common/integration/apps/common/tsconfig.app.json",
                        "main": "packages/common/integration/apps/common/main.ts",
                        "index": "packages/common/integration/apps/common/index.html",
                        "assets": ["packages/common/integration/apps/common/assets"],
                        "polyfills": "packages/common/integration/apps/common/polyfills.ts",
                        "styles": [
                            ".styles/integration-app.style.scss",
                            "packages/common/integration/apps/common/styles.scss"
                        ],
                        "aot": false,
                        "vendorChunk": true,
                        "extractLicenses": false,
                        "buildOptimizer": false,
                        "sourceMap": true,
                        "optimization": false,
                        "namedChunks": true
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "packages/common/integration/apps/common/environments/environment.ts",
                                    "with": "packages/common/integration/apps/common/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ]
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": { "browserTarget": "common-app:build" },
                    "configurations": {
                        "production": {
                            "browserTarget": "common-app:build:production"
                        }
                    }
                },
                "deploy": {
                    "builder": "angular-cli-ghpages:deploy",
                    "options": {
                        "noSilent": true,
                        "noBuild": true,
                        "branch": "master",
                        "name": "angular-ru",
                        "email": "angular-ru@yandex.ru",
                        "repo": "https://github.com/Angular-RU/angular-ru-common-example-app.git"
                    }
                }
            }
        },
